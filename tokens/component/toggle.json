              }
            }
          },
          "handle": {
            "default": {
              "bg": {
                "default": {
                  "value": "{app.color.surface.fg.inverted}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            },
            "disabled": {
              "bg": {
                "default": {
                  "value": "{app.color.surface.fg.inverted}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            }
          }
        },
        "checked": {
          "track": {
            "default": {
              "bg": {
                "default": {
                  "value": "{app.color.component.accent.bg.solid}",
                  "type": "color"
                },
                "active": {
                  "value": "{component.toggle.color.checked.track.default.bg.default}",
                  "type": "color",
                  "$extensions": {
                    "studio.tokens": {
                      "modify": {
                        "type": "darken",
                        "value": "{config.color.primary.darken.active}",
                        "space": "hsl"
                      }
                    }
                  }
                }
              },
              "fg": {
                "default": {
                  "value": "{app.color.component.accent.fg.onSolid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                },
                "active": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            },
            "disabled": {
              "bg": {
                "default": {
                  "value": "{app.color.component.accent.bg.solid}",
                  "type": "color"
                }
              },
              "fg": {
                "default": {
                  "value": "{app.color.surface.fg.inverted}",
                  "type": "color",
                  "$extensions": {
                    "studio.tokens": {
                      "modify": {
                        "type": "alpha",
                        "value": "0.3",
